load "test"

fn equal_type(a, b) {
  return introspect_type(a) == introspect_type(b)
}

fn a {
  let value = rand() % 5
  if value < 3 {
    return value
  }
  return nil
}

let val = a()
printf("val: %\n", val)

if equal_type(val, nil) {
  return "Nil"
}
if equal_type(val, 0) {
  return "Number"
}

