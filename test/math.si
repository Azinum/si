// math.si

fn math_isqrt(n) {
  let res = n;
  let xhalf = 0.5 * res;
  res = 0x5f3759df - (res >> 1);
  res = res * (1.5 - xhalf * res * res);
  return res;
}

fn math_sqrt(n) {
  let res = 1;

  let i = 0;
  while i < n {
    i = i + 1;
    res = 0.5 * (res + n / res);
  }
  return res;
}

fn math_fib(n) {
  if n <= 2 {
    return n;
  };
  return math_fib(n - 2) + math_fib(n - 1);
}